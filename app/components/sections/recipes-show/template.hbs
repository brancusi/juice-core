<div class="col-4_sm-12_md-6">
  {{ui/label-button
      class='back-button'
      label='< Back to recipes'
      onClick=(action navigateTo 'recipes')}}

  {{editors/recipe-editor
      handleUpdate=handleUpdate
      addNode=addNode
      deleteEdge=deleteEdge
